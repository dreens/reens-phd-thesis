journal = {Physical Review A},
month = {may},
number = {5},
pages = {052511},
title = {{Precision spectra of $A\, ^2\Sigma^+,v'=0 \leftarrow X\, ^2\Pi_{3/2},v''=0,J''=3/2$ transitions in $^{16}$OH and $^{16}$OD}},
