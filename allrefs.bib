issn = {03770486},
journal = {Journal of Raman Spectroscopy},
keywords = {Raman depolarization ratio,hydrogen isotopologues,tritium},
number = {6},
pages = {857--865},
title = {{Accurate depolarization ratio measurements for all diatomic hydrogen isotopologues}},
volume = {44},
year = {2013}
}
